{
  "name": "snabb",
  "version": "1.0.0",
  "description": "NA-KD single page application.",
  "main": "server/index.js",
  "scripts": {
    "start": "node_modules/babel-cli/bin/babel-node.js src/server",
    "start:production": "NODE_ENV=production docker-compose up",
    "build:container": "docker-compose build",
    "build:client": "node_modules/webpack/bin/webpack.js --config webpack.config.production.js --progress --profile",
    "analyze": "node_modules/webpack/bin/webpack.js -json | node_modules/webpack-bundle-size-analyzer/webpack-bundle-size-analyzer",
    "hmr": "NODE_ENV=development DEBUG=snabb* node_modules/babel-cli/bin/babel-node.js src/server",
    "test": "node_modules/.bin/nyc node_modules/mocha/bin/mocha --compilers js:babel-register --recursive --slow 12 --reporter nyan \"./src/**/tests/*-spec.js\"",
    "test:watch": "node_modules/mocha/bin/mocha \"./src/**/tests/*-spec.js\" --compilers js:babel-core/register --watch --slow 12",
    "test:coverage": "node_modules/.bin/nyc node_modules/.bin/mocha \"./src/**/tests/*-spec.js\"",
    "test:unit": "./node_modules/babel-cli/bin/babel-node.js ./scripts/test-single-unit.js",
    "deploy": "./deploy/deploy.sh -d -p -w",
    "flow": "node_modules/flow-bin/cli.js",
    "build:lang": "./node_modules/.bin/babel-node src/util/build-lang.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodjevlar/snabb.git"
