{
  "name": "snabb",
  "version": "0.2.1",
  "description": "Minimal react SPA which can be used in conjunction with the functional e-commerce repository.",
  "main": "server/index.js",
  "scripts": {
    "start": "NODE_ENV=production node build/server",
    "build:client": "node_modules/webpack/bin/webpack.js --config webpack.config.production.js -p --progress",
    "build:server": "NODE_ENV=production node_modules/babel-cli/bin/babel.js src/ -d build --copy-files",
    "build:all": "npm run build:server;npm run build:client",
    "analyze": "node_modules/webpack/bin/webpack.js -json | node_modules/webpack-bundle-size-analyzer/webpack-bundle-size-analyzer",
    "hmr": "NODE_ENV=development node_modules/babel-cli/bin/babel-node.js src/server",
    "test": "npm run test:nyc",
    "test:watch": "node_modules/mocha/bin/mocha \"./src/**/tests/*-spec.js\" --compilers js:babel-core/register --watch --recursive",
    "test:coverage": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- \"./src/**/tests/*-spec.js\" --compilers js:babel-core/register ",
    "test:nyc": "node_modules/.bin/nyc --require babel-core/register node_modules/.bin/mocha \"./src/**/tests/*-spec.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kodjevlar/snabb.git"
  },
  "author": "Joel snabbRoxell <joel.roxell@na-kd.com> (https://github.com/JoelRoxell)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kodjevlar/snabb/issues"
  },
  "homepage": "https://github.com/Kodjevlar/snabb#readme",
  "dependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-webpack-alias": "^2.1.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.16.3",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "generic-names": "^1.0.1",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.13.0",
    "postcss-modules-parser": "^1.1.0",
    "poststylus": "^0.2.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-dom-stream": "^0.5.1",
    "react-router": "^2.8.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-watch": "^2.0.3",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "clone": "^2.0.0",
    "eslint": "^3.7.1",
    "eslint-config-google": "^0.6.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "jquery": "^3.1.1",
    "jsdom": "^9.6.0",
    "mocha": "^3.1.2",
    "nyc": "^8.3.1",
    "postcss": "^5.2.4",
    "react-addons-test-utils": "^15.3.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.1",
    "webpack-bundle-size-analyzer": "^2.2.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  }
}
